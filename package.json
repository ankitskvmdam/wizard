{
  "name": "wizard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "npx webpack --config webpack.config.js",
    "watchBuild": "npx webpack --watch --config webpack.config.js",
    "refreshServer": "npx nodemon ./bin/www",
    "dev": "concurrently \"npm run watchBuild\" \"npm run refreshServer\"",
    "cypress:open": "npx wait-on http://localhost:9992 && npx cypress open",
    "docker-compose": "docker-compose up --build --force-recreate",
    "cypress:test": "concurrently  --kill-others-on-fail 'npm run docker-compose' 'npm run cypress:open'",
    "pretest": "npx eslint --fix --ignore-path .gitignore .",
    "generate-docs": "npx jsdoc -c jsdoc.json && open-cli ./docs/index.html",
    "lint": "eslint .",
    "lint-fix": "eslint --fix ."
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsdoc": "^3.6.3",
    "less-middleware": "~2.2.1",
    "morgan": "~1.9.1",
    "open-cli": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "cypress": "^3.4.1",
    "eslint": "^6.3.0",
    "faker": "^4.1.0",
    "nodemon": "^1.19.1",
    "postman-jsdoc-theme": "0.0.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6"
  }
}
